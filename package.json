{
  "private": true,
  "scripts": {
    "start": "npm run build && parallelshell 'npm run serve' 'npm run watch'",
    "serve": "browser-sync start -s --files icons.svg,index.html,app.css,app.js,hp/*.html,srp/*.html,reg/*.*,signin/*.*",
    "build": "lasso browser.json --minify --out . --name app --html false",
    "watch": "onchange main.js -- npm run build"
  },
  "devDependencies": {
    "browser-sync": "^2",
    "jquery": ">=1.11",
    "jquery-click-flyout": "latest",
    "jquery-common-keydown": "^1.0.1",
    "jquery-focus-exit": "^1.0.1",
    "jquery-focus-flyout": "^0.0.5",
    "jquery-focusable": "latest",
    "jquery-grid-navigation": "^1.0.1",
    "jquery-hover-flyout": "^0.0.5",
    "jquery-linear-navigation": "^1.0.2",
    "jquery-menu": "^0.7.3",
    "jquery-mouse-exit": "^1.0.1",
    "jquery-next-id": "^1.0.1",
    "jquery-prevent-scroll-keys": "^1.0.1",
    "jquery-roving-tabindex": "^1.0.1",
    "jquery-skip-to": "^0.0.2",
    "jquery-tooltip": "^0.2.1",
    "lasso-cli": "^2",
    "onchange": "^5",
    "parallelshell": "^3"
  }
}
