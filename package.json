{
  "private": true,
  "scripts": {
    "start": "npm run build && parallelshell 'npm run serve' 'npm run watch'",
    "serve": "browser-sync start -s --files index.html,app.css,app.js,hp/*.html,srp/*.html,reg/*.html/signin/*.html",
    "build": "lasso browser.json --minify --out . --name app --html false",
    "watch": "onchange main.js -- npm run build"
  },
  "devDependencies": {
    "browser-sync": "^2",
    "jquery": ">=1.11",
    "jquery-click-flyout": "latest",
    "jquery-common-keydown": "latest",
    "jquery-focusable": "latest",
    "jquery-focus-exit": "latest",
    "jquery-focus-flyout": "latest",
    "jquery-grid-navigation": "latest",
    "jquery-hover-flyout": "latest",
    "jquery-linear-navigation": "latest",
    "jquery-menu": "latest",
    "jquery-mouse-exit": "latest",
    "jquery-next-id": "latest",
    "jquery-prevent-scroll-keys": "latest",
    "jquery-roving-tabindex": "latest",
    "jquery-skip-to": "latest",
    "jquery-tooltip": "latest",
    "lasso-cli": "^2",
    "onchange": "^5",
    "parallelshell": "^3"
  }
}
